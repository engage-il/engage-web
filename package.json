{
  "name": "engage-il",
  "version": "0.0.1",
  "description": "Web front-end for engage-il",
  "main": "dist/bundle.js",
  "author": "Ty Cobb",
  "license": "MIT",
  "scripts": {
    "start": "./node_modules/.bin/webpack-dev-server -d --hot --inline",
    "build": "./node_modules/.bin/webpack -p --optimize-occurrence-order --optimize-dedupe",
    "flow:restart": "flow stop && flow",
    "lint": "snazzy",
    "lint:fix": "snazzy --fix",
    "validate": "yarn lint && yarn flow"
  },
  "dependencies": {
    "lodash": "^4.17.2",
    "react": "^15.4.1",
    "react-dom": "^15.4.1"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.0.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.8",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.0",
    "flow-bin": "^0.42.0",
    "snazzy": "^6.0.0",
    "standard": "^9.0.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "flow-typed"
    ]
  }
}
